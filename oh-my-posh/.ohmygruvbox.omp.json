{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "diamond",
          "type": "path",
          "background": "#458588",
          "foreground": "#ebdbb2",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "properties": {
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "~",
            "style": "folder",
            "template": " \uf74a  {{ .Path }} "
          }
        },
        {
          "background": "#689d6a",
          "foreground": "#ebdbb2",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_version": true,
            "template": " \uf898 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} "
          },
          "style": "powerline",
          "type": "node"
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ebdbb2",
          "background": "#83a598",
          "properties": {
            "template": "   {{ .Full }} "
          }
        },
        {
          "background": "#ebdbb2",
          "foreground": "#689d6a",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_version": true,
            "template": " \ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
          },
          "style": "powerline",
          "type": "go"
        },
        {
          "background": "#ebdbb2",
          "foreground": "#b16286",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false,
            "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
          },
          "style": "powerline",
          "type": "python"
        },
        {
          "background": "#b16286",
          "foreground": "#ebdbb2",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "template": " \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
          },
          "style": "powerline",
          "type": "ruby"
        },
        {
          "background": "#83a598",
          "foreground": "#ebdbb2",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_mode": "files",
            "fetch_version": false,
            "template": " \uf0e7{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
          },
          "style": "powerline",
          "type": "azfunc"
        },
        {
          "background_templates": [
            "{{if contains \"default\" .Profile}}#d65d0e{{end}}",
            "{{if contains \"jan\" .Profile}}#fe8019{{end}}"
          ],
          "foreground": "#ebdbb2",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_default": false,
            "template": " \ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} "
          },
          "style": "powerline",
          "type": "aws"
        },
        {
          "background": "#ebdbb2",
          "foreground": "#cc241d",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "template": " \uf0ad "
          },
          "style": "powerline",
          "type": "root"
        },
        {
          "background": "#b8bb26",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#fb4934{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#d79921{{ end }}",
            "{{ if gt .Ahead 0 }}#d79921{{ end }}",
            "{{ if gt .Behind 0 }}#d79921{{ end }}"
          ],
          "foreground": "#3c3836",
          "leading_diamond": "\ue0b6",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} "
          },
          "style": "powerline",
          "trailing_diamond": "\ue0b4",
          "type": "git"
        },
        {
          "background": "#689d6a",
          "background_templates": ["{{ if gt .Code 0 }}#fb4934{{ end }}"],
          "foreground": "#ebdbb2",
          "properties": {
            "always_enabled": true,
            "template": " \ue23a "
          },
          "style": "powerline",
          "powerline_symbol": "",
          "invert_powerline": false,
          "type": "exit"
        }
      ]
    }
  ],
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "version": 1
}
